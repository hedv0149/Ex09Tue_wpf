<Window x:Class="Ex09Tue_wpf.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Ex09Tue_wpf"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Frame x:Name="Main" Margin=" 0,35,0,0" NavigationUIVisibility="Hidden" />
        <StackPanel Orientation="Horizontal"  Height=" 35" VerticalAlignment="Top" >
            <Button x:Name="buttonPage1" Content="Page 1" MinWidth="100" Click="buttonPage1_Click"/>
        </StackPanel>
        <StackPanel Orientation="Horizontal"  Height=" 35" VerticalAlignment="Top" >
            <Button x:Name="buttonPage2" Content="Page 2"  MinWidth="100" Margin="418,0,0,0.2" Click="buttonPage2_Click"/>
        </StackPanel>
    </Grid>
</Window>
